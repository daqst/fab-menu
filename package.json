{
  "name": "fab-menu",
  "version": "1.0.0",
  "description": "Floating Action Button used to control a popup menu.",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@material/elevation": "^0.36.1",
    "@material/list": "^0.37.1",
    "@stencil/core": "^0.10.0",
    "@stencil/sass": "0.0.5",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fab-menu/"
  },
  "author": "David Quast",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daqst/fab-menu"
  },
  "homepage": "https://github.com/daqst/fab-menu",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
